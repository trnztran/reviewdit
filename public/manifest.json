{
    "manifest_version": 3,
    "name":"Reviewdit",
    "description": "Review aggregator for Amazon using Reddit",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_title": ":))"
    },
    "permissions": [
      "scripting",
      "activeTab",
      "tabs"
  ],
    "host_permissions": [
      "http://*/",
      "https://*/"
    ]
  }